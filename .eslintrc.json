{
    "env": {
        "shared-node-browser": true,
        "commonjs": true,
        "browser": true,
        "es6": true,
        "jquery": true,
        "node": true
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}